{
	"extends": ["development"],
	"hints": {
		"apple-touch-icons": "off",
		"disown-opener": "off",
		"compat-api/css": [
			"default",
			{
				"ignore": [
					"user-select",
					"backdrop-filter",
					"scrollbar-color",
					"scrollbar-width",
					"scrollbar-width: thin",
					"min-height: auto"
				]
			}
		],
		"axe/name-role-value": [
			"default",
			{
				"button-name": "off"
			}
		],
		"no-inline-styles": "off"
	}
}
